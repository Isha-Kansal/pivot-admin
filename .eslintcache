() => {\n    currentPage !== page && setPage(currentPage);\n  }, [currentPage, page]);\n  useEffect(() => {\n    dispatch(\n      fetchUsers(\"user/all\", (value) => {\n        setUsersDetails(value.data.users);\n      })\n    );\n  }, []);\n  const filterRecords = () => {\n    // const search = search.trim().replace(/ +/g, \" \");\n    if (!search) return usersDetails;\n    return (\n      usersDetails &&\n      usersDetails.filter((data) => {\n        let isTrue =\n          data.email.toLowerCase().includes(search) ||\n          data.country.toLowerCase().includes(search) ||\n          data.name.toLowerCase().includes(search);\n        return isTrue;\n      })\n    );\n  };\n  const getBadge = (status) => {\n    switch (status) {\n      case \"Verified\":\n        return \"success\";\n\n      case \"Not Verified\":\n        return \"danger\";\n      default:\n        return \"primary\";\n    }\n  };\n  const searchRecords = filterRecords();\n\n  const onBlock = (e) => {\n    e.preventDefault();\n    e.stopPropagation();\n    alert(\"user Blocked\");\n  };\n\n  return (\n    <CRow>\n      <CCol xl={12}>\n        <form>\n          <div className=\"text-center search-input\">\n            <input\n              type=\"search\"\n              className=\"form-control\"\n              placeholder=\"Search\"\n              required\n              onChange={handleSearch}\n            />\n          </div>\n        </form>\n      </CCol>\n\n      <CCol xl={12}>\n        <CCard>\n          <CCardHeader>Users</CCardHeader>\n          <CCardBody>\n            <CDataTable\n              items={searchRecords}\n              fields={[\n                { key: \"name\", _classes: \"font-weight-bold\" },\n                \"email\",\n                \"status\",\n                \"country\",\n                \"action\",\n              ]}\n              scopedSlots={{\n                name: (item) => <td>{item.name ? item.name : \"-\"}</td>,\n                email: (item) => <td>{item.email ? item.email : \"-\"}</td>,\n                status: (item) => (\n                  <td>\n                    <CBadge\n                      color={getBadge(\n                        !item.is_verified ? \"Not Verified\" : \"Verified\"\n                      )}\n                    >\n                      {!item.is_verified ? \"Not Verified\" : \"Verified\"}\n                    </CBadge>\n                  </td>\n                ),\n                country: (item) => <td>{item.country ? item.country : \"-\"}</td>,\n                action: (item) => (\n                  <td>\n                    <CButton\n                      // style={{ backgroundColor: \"rgb(200,200,200)\" }}\n                      onClick={onBlock}\n                      className=\"block-btn block-btn\"\n                    >\n                      Block\n                    </CButton>\n                    <CButton\n                      // style={{ backgroundColor: \"rgb(200,200,200)\" }}\n                      onClick={onBlock}\n                      className=\"Unblock-btn block-btn\"\n                    >\n                      UnBlock\n                    </CButton>\n                    <CButton\n                      // style={{ backgroundColor: \"rgb(200,200,200)\" }}\n                      onClick={onBlock}\n                      className=\"Deactive-btn block-btn\"\n                    >\n                      Deactivate\n                    </CButton>\n                  </td>\n                ),\n              }}\n              itemsPerPage={10}\n              activePage={page}\n              clickableRows\n              onRowClick={(item) =>\n                history.push({\n                  pathname: `/users/${item._id}`,\n                  state: usersDetails,\n                })\n              }\n            />\n\n            <div className=\"text-center pagination-input\">\n              {usersDetails.length > 10 && (\n                <Pagination\n                  classN